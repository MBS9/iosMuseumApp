<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="museumApp.Other"
             Title="Other Museums">
    <VerticalStackLayout>
        <Label 
            Text="Other Museums"
            FontSize="Title"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Image
            Source="museum_large.png"
            HeightRequest="100"
            HorizontalOptions="Center" />
        <Button x:Name="exampleMuseumBtn" Text="Example Other Museum" Clicked="exampleMuseumBtn_Clicked"/>
    </VerticalStackLayout>
</ContentPage>